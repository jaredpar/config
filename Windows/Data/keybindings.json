// Place your key bindings in this file to override the defaults
[
    {
        "key": "ctrl+shift+t",
        "command": "workbench.action.terminal.focus"
    },
    {
        "key": "ctrl+shift+c",
        "command": "workbench.action.showCommands"
    },
    {
        "key": "ctrl+shift+p",
        "command": "-workbench.action.showCommands"
    },
    {
        "key": "ctrl+shift+l",
        "command": "workbench.action.tasks.runTask"
    }
]